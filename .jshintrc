{
  "camelcase": true,
  "freeze": true,
  "indent": 2,
  "curly": true,
  "latedef": false,
  "newcap": true,
  "noempty": true,
  "plusplus": true,
  "undef": true,
  "unused": true,
  "predef": ["console", "require", "requirejs", "define", "window", "document", "module"],
  "trailing": true,
  "quotmark": "single",
  "eqeqeq": true,
  "maxlen": 0,
  "maxcomplexity": 10,
  "noarg": true,
  "immed": true,
  "node": true
}